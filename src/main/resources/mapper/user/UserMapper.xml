<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.jseckill.infrastructure.biz.user.db.dao.UserDao">

    <select id="getByUsername" resultType="com.example.jseckill.infrastructure.biz.user.db.po.User">
        select t1.* from t_user t1
        <where>
            t1.username = #{username}
        </where>
    </select>
</mapper>
